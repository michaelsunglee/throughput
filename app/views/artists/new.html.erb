<%= stylesheet_link_tag "artists", media: "all", "data-turbolinks-track" => true %>
<%= render 'artist_form', artist: @artist %>
<div class="page-results">
  <% @searched.each do |search| %>
    <%= link_to search_path(:id => search.id) do %>
        <% image_tag search.image, width: 320, height: 320, border: 0 %>
    <% end %>
    <p class="text-white new-score">
      <strong>Score: </strong><%= search.score %>
    </p>
  <% end %>
</div>
